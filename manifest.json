{
  "manifest_version": 2,
  "name": "Youtube Playlist Analyser",
  "version": "1.0.0",
  "web_accessible_resources": ["js/script.js"],
  "content_scripts": [
    {"matches": ["*://*.youtube.com/*"],
    "js":  ["js/Content.js"],
      "run_at": "document_start"}
  ],
  "icons": {
    "16": "assets/16x16.png",
    "48": "assets/48x48.png",
    "128": "assets/128x128.png"
  }
}